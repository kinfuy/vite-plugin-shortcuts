  "devDependencies": {
    "@alqmc/build-ts": "^0.0.9",
    "@alqmc/build-utils": "^0.0.5",
    "@alqmc/eslint-config": "^0.0.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/gulp": "^4.0.9",
    "changeloger": "0.1.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^18.11.12",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "picocolors": "^1.0.0",
    "open": "^8.4.0",
    "cross-spawn": "^7.0.3"
  },
  "version": "0.0.5"
